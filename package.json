{
    "name": "cora-case",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "api": "json-server --watch db.json --port 4000",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn prettier-format && yarn lint"
        }
    },
    "dependencies": {
        "axios": "^0.27.2",
        "formik": "^2.2.9",
        "husky": "^8.0.1",
        "json-server": "^0.17.0",
        "prettier": "^2.7.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^4.4.0",
        "react-router-dom": "^6.3.0",
        "react-toastify": "^9.0.5",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@vitejs/plugin-react": "^1.3.0",
        "autoprefixer": "^10.4.7",
        "postcss": "^8.4.14",
        "tailwindcss": "^3.1.4",
        "typescript": "^4.6.3",
        "vite": "^2.9.9"
    }
}
